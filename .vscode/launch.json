{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run ProyectoFinal-ProyectoFinal",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/proyectofinal-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/ProyectoFinalProyectoFinal/index.html",
        "--ui5",
        "{\"version\":\"1.87.1\"}"
      ],
      "cwd": "${workspaceFolder}/ProyectoFinal",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/ProyectoFinal/ProyectoFinal\"}"
      },
      "envFile": "${workspaceFolder}/ProyectoFinal/.env1",
      "preLaunchTask": "Build ProyectoFinal"
    }
  ]
}